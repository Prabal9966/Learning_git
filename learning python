print("if area of triangle press=1,if area of rectangle press=2,if area of cube  press=3")   
lenght=float(input("Enter the lenght="))
breadth=float(input("Enter the breadth="))
if num==1:
num1=1/2*lenght*breadth
print("Area of triangle=",num1)
elif num==2:
num2=lenght*breadth
print("Area of rectangle=",num2)
else:
side=float(input("Enter the side of cube="))
num3=side*side
print("Area of cube=",num3)
